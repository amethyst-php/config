{
    "name": "railken/amethyst-config",
    "description": "Amethyst package",
    "keywords": ["laravel", "amethyst", "amethyst-config", "config"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "fzaninotto/faker": "^1.7",
        "railken/bag": "^1.2",
        "railken/amethyst-api": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10",
        "phpunit/phpunit": "~7.1",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8.1"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\ConfigServiceProvider"
            ]
        }
    }
}
